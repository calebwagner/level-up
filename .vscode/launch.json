{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Request Handler",
      "type": "python",
      "request": "launch",
      "program": "./request_handler.py",
      "console": "integratedTerminal",
      "autoReload": {
        "enable": true
      }
    },
    {
      "name": "Django Handler",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/manage.py",
      "args": ["runserver"],
      "autoReload": {
        "enable": true
      },
      "django": true
    }
  ]
}
